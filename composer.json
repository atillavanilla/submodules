{
    "name": "atilla/submodules",
    "description": "A Laravel package for generating and managing modular API applications",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^2.36",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.4",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Atilla\\SubmoduleGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atilla\\SubmoduleGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Atilla\\SubmoduleGenerator\\SubmoduleGeneratorServiceProvider"
            ],
            "aliases": {
                "SubmoduleGenerator": "Atilla\\SubmoduleGenerator\\Facades\\SubmoduleGenerator"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
